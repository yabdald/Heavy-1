<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CompanyHomePage">

    >

    <LinearLayout
        android:id="@+id/linearLayout7"
        android:layout_width="349dp"
        android:layout_height="138dp"
        android:layout_margin="20dp"
        android:layout_weight="1"
        android:background="#1687A7"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.28"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.717">

        <ImageView
            android:layout_width="79dp"
            android:layout_height="80dp"
            android:src="@drawable/account_management"
            app:srcCompat="@drawable/income" />

        <TextView
            android:layout_width="122dp"
            android:layout_height="46dp"
            android:layout_marginBottom="10dp"
            android:gravity="center"
            android:text="Incomes"
            android:textColor="#ffff"
            android:textSize="25dp"
            android:textStyle="normal" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="170dp"
        android:layout_height="146dp"
        android:layout_margin="20dp"
        android:layout_weight="1"
        android:background="#1687A7"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.933"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.097">

        <ImageView
            android:layout_width="70dp"
            android:layout_height="87dp"
            android:src="@drawable/account_management"
            app:srcCompat="@drawable/add_user" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:layout_marginBottom="10dp"
            android:gravity="center"
            android:text="Add User"
            android:textColor="#ffff"
            android:textSize="25dp"
            android:textStyle="normal" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="170dp"
        android:layout_height="146dp"
        android:layout_margin="20dp"
        android:layout_weight="1"
        android:background="#1687A7"
        android:gravity="center"
        android:orientation="vertical"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.097">

        <ImageView
            android:layout_width="70dp"
            android:layout_height="91dp"
            android:src="@drawable/account_management"
            app:srcCompat="@drawable/account_management" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:layout_marginBottom="10dp"
            android:gravity="center"
            android:text="Mange Driver"
            android:textColor="#ffff"
            android:textSize="25dp"
            android:textStyle="normal" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout10"
        android:layout_width="170dp"
        android:layout_height="141dp"
        android:layout_margin="20dp"
        android:layout_weight="1"
        android:background="#1687A7"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.934"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.404">

        <ImageView
            android:layout_width="70dp"
            android:layout_height="87dp"
            android:src="@drawable/account_management"
            app:srcCompat="@drawable/product" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:layout_marginBottom="10dp"
            android:gravity="center"
            android:text="Orders"
            android:textColor="#ffff"
            android:textSize="25dp"
            android:textStyle="normal" />
    </LinearLayout>

    <LinearLayout

        android:layout_width="fill_parent"
        android:layout_height="90dp"
        android:layout_gravity="bottom"

        android:background="#1687A7"
        android:gravity="left"

        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout7"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp">

        <ImageView
            android:id="@+id/homeIcon"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_gravity="center"
            android:layout_marginLeft="45dp"
            android:src="@drawable/home" />

        <ImageView
            android:id="@+id/profileIcone"
            android:layout_width="80dp"
            android:layout_height="70dp"
            android:layout_gravity="center"
            android:layout_marginLeft="190dp"
            android:src="@drawable/user_icone" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="170dp"
        android:layout_height="146dp"
        android:layout_margin="20dp"
        android:layout_weight="1"
        android:background="#1687A7"
        android:gravity="center"
        android:orientation="vertical"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.405">

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="match_parent"
            android:layout_height="91dp"
            android:src="@drawable/account_management"
            app:srcCompat="@drawable/trucks" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:layout_marginBottom="10dp"
            android:gravity="center"
            android:text="Trucks"
            android:textColor="#ffff"
            android:textSize="25dp"
            android:textStyle="normal" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>